<br>
<ul class="list-unstyled">
    <% if (user) { %>
        <% for (var i = 0; i < user.local.goals.length; i++) { %>
            <li class="d-flex justify-content-between">
                <%=  user.local.goals[i].goal %>
                <p><span class="text-info">Edit</span> | <a href="/profile/<%= user.local.goals[i]._id %>" style="text-decoration:none;"><span class="text-danger">Delete</span></a></p>
            </li>
            <hr>
        <% } %>
    <% } %>
</ul>
